@{
}
<h3>Menu Manager</h3>
<table>
    <thead>
        <tr>
            <th>Menu Name</th>
            <th>Menu Price</th>
            
            
        </tr>
    </thead>
    <tbody>
        @foreach (menusDTO menuDTO in Model.menu)
        {
            <tr>
                <td>@menuDTO.itemname</td>
                <td>@menuDTO.price</td>
                
                


</tr>
}
</tbody>
</table>